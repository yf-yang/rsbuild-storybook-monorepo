{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "WebWorker",
      "ES2020"
    ],
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "moduleResolution": "bundler",
    "composite": true,
    "useDefineForClassFields": true
  },
  "include": [
    "src"
  ]
}